{
  "name": "jitap-types",
  "main": "dist/lib.cjs.js",
  "module": "dist/lib.esm.js",
  "version": "0.0.1",
  "typings": "./dist/typings/Lib.d.ts",
  "description": "JiTap - Types",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm-run-all clean rollup:build typings:emit",
    "rollup:build": "cross-env NODE_ENV=production rollup -c",
    "typings:emit": "tsc --emitDeclarationOnly true",
    "schema:tt": "typescript-json-schema --ignoreErrors 'TinyTap.d.ts' Game",
    "schema:ji": "typescript-json-schema --ignoreErrors 'JiTap.d.ts' Slide"
  },
  "author": "David Komer / Jewish Interactive",
  "license": "UNLICENSED",
  "devDependencies": {
    "cross-env": "5.2.0",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "rollup-plugin-typescript2": "0.16.1",
    "typescript": "3.0.1"
  }
}
